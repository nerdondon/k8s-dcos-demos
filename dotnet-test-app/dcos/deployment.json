{
    "id": "dotnet-test-app",
    "cpus": 0.1,
    "mem": 90.0,
    "instances": 3,
    "acceptedResourceRoles": [
        "slave_public"
    ],
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "nerdondon/dotnet-test-app:v2",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "healthChecks": [
        {
            "portIndex": 0,
            "protocol": "MESOS_HTTP",
            "gracePeriodSeconds": 300,
            "intervalSeconds": 60,
            "timeoutSeconds": 20,
            "maxConsecutiveFailures": 3
        }
    ]
}